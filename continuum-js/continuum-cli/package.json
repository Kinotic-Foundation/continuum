{
  "name": "@kinotic/continuum-cli",
  "version": "0.1.0",
  "description": "Continuum Framework CLI",
  "author": "Navid Mitchell @NavidMitchell",
  "bin": {
    "continuum": "./bin/run.js"
  },
  "homepage": "https://github.com/Kinotic-Foundation/continuum-cli",
  "license": "MIT",
  "exports": "./dist/index.js",
  "repository": "Kinotic-Foundation/continuum-cli",
  "type": "module",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-autocomplete": "^2.1.4",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.21",
    "@oclif/plugin-plugins": "^2.3.2",
    "@oclif/plugin-update": "^3.1.6",
    "@oclif/plugin-warn-if-update-available": "^2.0.30",
    "execa": "^7.0.0",
    "inquirer": "^9.1.4",
    "liquidjs": "^10.6.1",
    "lodash-es": "^4.17.21",
    "make-dir": "^3.1.0",
    "simple-git": "^3.17.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.8",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.3",
    "@types/json-schema": "^7.0.11",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.14",
    "@types/lodash-es": "^4.17.6",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "continuum",
    "dirname": "continuum",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "create": {
        "description": "Creates Continuum projects, applications, and services"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/Kinotic-Foundation/continuum-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
