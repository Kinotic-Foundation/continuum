{
    "name": "@kinotic/continuum-cli",
    "version": "0.1.0",
    "description": "Continuum Framework CLI",
    "author": "Navid Mitchell @NavidMitchell",
    "bin": {
        "continuum": "./bin/run.js"
    },
    "homepage": "https://github.com/Kinotic-Foundation/continuum-cli",
    "license": "MIT",
    "exports": "./dist/index.js",
    "repository": "Kinotic-Foundation/continuum-cli",
    "type": "module",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@oclif/core": "^2.8.10",
        "@oclif/plugin-autocomplete": "^2.3.1",
        "@oclif/plugin-help": "^5",
        "@oclif/plugin-not-found": "^2.3.26",
        "@oclif/plugin-plugins": "^2.4.7",
        "@oclif/plugin-update": "^3.1.21",
        "@oclif/plugin-warn-if-update-available": "^2.0.40",
        "execa": "^7.0.0",
        "inquirer": "^9.2.8",
        "liquidjs": "^10.6.1",
        "lodash-es": "^4.17.21",
        "make-dir": "^3.1.0",
        "simple-git": "^3.17.0"
    },
    "devDependencies": {
        "@oclif/test": "^2.3.23",
        "@types/chai": "^4",
        "@types/inquirer": "^9.0.3",
        "@types/json-schema": "^7.0.12",
        "@types/lodash-es": "^4.17.8",
        "@types/mocha": "^9.0.0",
        "@types/node": "^18.15.11",
        "@types/uuid": "^9.0.2",
        "@types/ws": "^8.5.5",
        "chai": "^4",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1.0.3",
        "mocha": "^9",
        "oclif": "^3",
        "shx": "^0.3.3",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.3",
        "typescript": "^5.0.2"
    },
    "oclif": {
        "bin": "continuum",
        "dirname": "continuum",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins",
            "@oclif/plugin-update",
            "@oclif/plugin-not-found",
            "@oclif/plugin-warn-if-update-available",
            "@oclif/plugin-autocomplete"
        ],
        "topicSeparator": " ",
        "topics": {
            "create": {
                "description": "Creates Continuum projects, applications, and services"
            }
        }
    },
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "bugs": "https://github.com/Kinotic-Foundation/continuum-cli/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts"
}
